// C++ code
//
void setup()
{
  pinMode(13, OUTPUT);
  pinMode(11, OUTPUT);
}

void loop()
{
  digitalWrite(13, HIGH);
  digitalWrite(11, HIGH);
  //digitalWrite(11, LOW);
  delay(300); // Wait for 1000 millisecond(s)
  digitalWrite(13, LOW);
  digitalWrite(11, LOW);
  //digitalWrite(11, HIGH);
  delay(300); // Wait for 1000 millisecond(s)
}
